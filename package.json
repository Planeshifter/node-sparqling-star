{
  "name": "sparqling-star",
  "version": "1.1.0",
  "description": "client for creating SPARQL queries and communicating with services like DBpedia",
  "main": "lib/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Planeshifter/node-sparqling-star.git"
  },
  "keywords": [
    "SPARQL",
    "RDF",
    "semantic-web",
    "ontology",
    "DBpedia"
  ],
  "author": "Philipp Burckhardt",
  "license": "MIT",
  "readmeFilename": "Readme.md",
  "gitHead": "c9ab3e2fdf4bf717ca768544f8dbd1db9e89e5b9",
  "bugs": {
    "url": "https://github.com/Planeshifter/node-sparqling-star/issues"
  },
  "dependencies": {
    "array-intersection": "^0.1.2",
    "lodash.includes": "^4.1.0",
    "request": "^2.39.0",
    "underscore": "^1.6.0",
    "validate.io-object": "^1.0.4",
    "validate.io-string": "^1.0.2"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.4.2",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x",
    "mocha": "^2.2.4"
  }
}
